-- Scripts:

local function JHMZNES_fake_script() -- Main.LocalScript 
	local script = Instance.new('LocalScript', Main)

	-- Frame Pages
	local espFrame = script.Parent.ESPPage
	local mainFrame = script.Parent.MainFrame
	local mainPage = script.Parent.MainPage
	local morePage = script.Parent.More
	
	-- Elements
	local Section = mainFrame:WaitForChild("Section")
	local Button = Section:WaitForChild("Main")
	local Button2 = Section:WaitForChild("Esp")
	local Back = mainPage:WaitForChild("Back")
	local Back2 = espFrame:WaitForChild("Back")
	local Back3 = morePage:WaitForChild("Back")
	local changelog = mainFrame:WaitForChild("Changelog")
	local More = Section:WaitForChild("More")
	
	-- UI Elements
	local aimTargetTextMain = mainPage:WaitForChild("AimTargetLabel")
	local aimTextMain = mainPage:WaitForChild("TextLabel")
	local aimTargetTextMore = morePage:WaitForChild("AimTargetLabel")
	local aimTextMore = morePage:WaitForChild("TextLabel")
	local aimTargetToggle = mainPage:WaitForChild("aimTargetToggle")
	local aimLockButton = morePage:WaitForChild("AimlockNPC")  -- Button for NPC Aimlock in MorePage
	
	local UserInputService = game:GetService("UserInputService")
	local RunService = game:GetService("RunService")
	local camera = workspace.CurrentCamera
	local player = game.Players.LocalPlayer
	local players = game:GetService("Players")
	
	local guiVisible = true
	local isAimLockOn = false
	local isNPCAimLockOn = false -- For NPC AimLock
	local isHoldingE = false
	local isESPOn = false
	local aimTarget = "Head" -- Default aim target
	
	-- UI Elements Setup
	aimTargetTextMain.Text = "Target: Head"
	aimTextMain.Text = "OFF"
	aimTargetTextMore.Text = "Target: Head"
	aimTextMore.Text = "OFF"
	
	-- Toggle Aim Target (MainPage)
	aimTargetToggle.MouseButton1Click:Connect(function()
		aimTarget = (aimTarget == "Head") and "Neck" or (aimTarget == "Neck" and "Torso" or "Head")
		aimTargetTextMain.Text = "Target: " .. aimTarget
		aimTargetTextMore.Text = "Target: " .. aimTarget
	end)
	
	-- Toggle NPC AimLock (MorePage)
	aimLockButton.MouseButton1Click:Connect(function()
		isNPCAimLockOn = not isNPCAimLockOn
		aimTextMore.Text = isNPCAimLockOn and "ON" or "OFF"
	end)
	
	-- Toggle MainPage AimLock
	local aimLockButtonMain = mainPage:WaitForChild("AimlockButtonMain") -- Assuming there's a button for Aimlock in MainPage
	aimLockButtonMain.MouseButton1Click:Connect(function()
		isAimLockOn = not isAimLockOn
		aimTextMain.Text = isAimLockOn and "ON" or "OFF"
	end)
	
	-- Toggle GUI Visibility
	Button.MouseButton1Click:Connect(function()
		mainFrame.Visible = not mainFrame.Visible
		mainPage.Visible = not mainPage.Visible
		changelog.Visible = not changelog.Visible
	end)
	
	More.MouseButton1Click:Connect(function()
		mainFrame.Visible = not mainFrame.Visible
		morePage.Visible = not morePage.Visible
		changelog.Visible = not changelog.Visible
	end)
	
	Button2.MouseButton1Click:Connect(function()
		mainFrame.Visible = not mainFrame.Visible
		espFrame.Visible = not espFrame.Visible
		changelog.Visible = not changelog.Visible
	end)
	
	Back.MouseButton1Click:Connect(function()
		mainPage.Visible = not mainPage.Visible
		mainFrame.Visible = not mainFrame.Visible
		changelog.Visible = not changelog.Visible
	end)
	
	Back2.MouseButton1Click:Connect(function()
		mainFrame.Visible = not mainFrame.Visible
		espFrame.Visible = not espFrame.Visible
		changelog.Visible = not changelog.Visible
	end)
	
	Back3.MouseButton1Click:Connect(function()
		mainFrame.Visible = not mainFrame.Visible
		morePage.Visible = not morePage.Visible
		changelog.Visible = not changelog.Visible
	end)
	
	-- AimLock Toggle Logic
	local function GetClosestTarget()
		local closestTarget = nil
		local shortestDistance = math.huge
	
		for _, enemy in pairs(players:GetPlayers()) do
			if enemy ~= player and enemy.Character and enemy.Character:FindFirstChild("HumanoidRootPart") then
				local targetPart
	
				if aimTarget == "Head" then
					targetPart = enemy.Character:FindFirstChild("Head")
				elseif aimTarget == "Torso" then
					targetPart = enemy.Character:FindFirstChild("UpperTorso") 
						or enemy.Character:FindFirstChild("Torso") 
						or enemy.Character:FindFirstChild("Chest") 
						or enemy.Character:FindFirstChild("LowerTorso") 
				elseif aimTarget == "Neck" then
					local head = enemy.Character:FindFirstChild("Head")
					local upperTorso = enemy.Character:FindFirstChild("UpperTorso") or enemy.Character:FindFirstChild("Torso")
	
					if head and upperTorso then
						targetPart = Instance.new("Part")
						targetPart.Size = Vector3.new(0.1, 0.1, 0.1) -- Tiny invisible part
						targetPart.Position = (head.Position + upperTorso.Position) / 2
						targetPart.Transparency = 1
						targetPart.Anchored = true
						targetPart.Parent = workspace
						game:GetService("Debris"):AddItem(targetPart, 0.1)
					elseif upperTorso then
						targetPart = upperTorso
					end
				end
	
				if targetPart and targetPart:IsA("BasePart") then
					local screenPosition, onScreen = camera:WorldToViewportPoint(targetPart.Position)
					if onScreen then
						local distance = (Vector2.new(screenPosition.X, screenPosition.Y) - UserInputService:GetMouseLocation()).Magnitude
						if distance < shortestDistance then
							shortestDistance = distance
							closestTarget = targetPart
						end
					end
				end
			end
		end
		return closestTarget
	end
	
	-- NPC AimLock Logic
	local function GetClosestNPC()
		local closestTarget = nil
		local shortestDistance = math.huge
	
		for _, npc in pairs(workspace:GetChildren()) do
			if npc:IsA("Model") and npc:FindFirstChild("Humanoid") and npc:FindFirstChild("HumanoidRootPart") then
				local targetPart = npc:FindFirstChild(aimTarget)
				if targetPart then
					local screenPosition, onScreen = camera:WorldToViewportPoint(targetPart.Position)
					if onScreen then
						local distance = (Vector2.new(screenPosition.X, screenPosition.Y) - UserInputService:GetMouseLocation()).Magnitude
						if distance < shortestDistance then
							shortestDistance = distance
							closestTarget = targetPart
						end
					end
				end
			end
		end
		return closestTarget
	end
	
	-- Keybind Controls
	UserInputService.InputBegan:Connect(function(input, gameProcessed)
		if not gameProcessed then
			if input.KeyCode == Enum.KeyCode.M then
				isAimLockOn = not isAimLockOn
				aimTextMain.Text = isAimLockOn and "ON" or "OFF"
			elseif input.KeyCode == Enum.KeyCode.L then
				aimTarget = (aimTarget == "Head") and "Neck" or (aimTarget == "Neck" and "Torso" or "Head")
				aimTargetTextMain.Text = "Target: " .. aimTarget
				aimTargetTextMore.Text = "Target: " .. aimTarget
			elseif input.KeyCode == Enum.KeyCode.E and (isAimLockOn or isNPCAimLockOn) then
				isHoldingE = true
			end
		end
	end)
	
	UserInputService.InputEnded:Connect(function(input, gameProcessed)
		if not gameProcessed and input.KeyCode == Enum.KeyCode.E then
			isHoldingE = false
		end
	end)
	
	-- AimLock Logic for Player and NPC
	RunService.RenderStepped:Connect(function()
		if (isAimLockOn or isNPCAimLockOn) and isHoldingE then
			local target
			if isAimLockOn then
				target = GetClosestTarget()
			elseif isNPCAimLockOn then
				target = GetClosestNPC()
			end
			if target then
				camera.CFrame = CFrame.new(camera.CFrame.Position, target.Position)
			end
		end
	end)
	
	-- Notification on Load
	local StarterGui = game:GetService("StarterGui")
	
	StarterGui:SetCore("SendNotification", {
		Title = "Welcome to John's Aimlock";
		Text = "Script Loaded | v0.21";
		Duration = 5;
	})
	
end
coroutine.wrap(JHMZNES_fake_script)()
local function QVOOC_fake_script() -- AladiaUIv02.LocalScript 
	local script = Instance.new('LocalScript', AladiaUIv02)

	local UserInputService = game:GetService("UserInputService")
	
	local function makeDraggable(dragFrame, parentFrame)
		local dragging, dragInput, startPos, dragStart
	
		local function update(input)
			local delta = input.Position - dragStart
			parentFrame.Position = UDim2.new(
				startPos.X.Scale, startPos.X.Offset + delta.X,
				startPos.Y.Scale, startPos.Y.Offset + delta.Y
			)
		end
	
		dragFrame.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
				dragging = true
				dragStart = input.Position
				startPos = parentFrame.Position
	
				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragging = false
					end
				end)
			end
		end)
	
		dragFrame.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
				dragInput = input
			end
		end)
	
		UserInputService.InputChanged:Connect(function(input)
			if dragging and input == dragInput then
				update(input)
			end
		end)
	end
	
	-- Automatically find all GUI elements with a "Drag" frame
	local screenGui = script.Parent
	
	for _, gui in pairs(screenGui:GetChildren()) do
		if gui:IsA("Frame") or gui:IsA("ImageLabel") then
			local dragFrame = gui:FindFirstChild("Drag")
			if dragFrame then
				makeDraggable(dragFrame, gui)
			end
		end
	end
	
end
coroutine.wrap(QVOOC_fake_script)()
local function KVZOAGD_fake_script() -- AladiaUIv02.Login 
	local script = Instance.new('LocalScript', AladiaUIv02)

	local TweenService = game:GetService("TweenService")
	local loginFrame = script.Parent:WaitForChild("LoginFrame")
	local mainFrame = script.Parent:WaitForChild("Main")
	
	local usernameBox = loginFrame:WaitForChild("UsernameBox")
	local passwordBox = loginFrame:WaitForChild("PasswordBox")
	local loginButton = loginFrame:WaitForChild("LoginButton")
	local errorLabel = loginFrame:WaitForChild("ErrorLabel")
	local welcome = loginFrame:WaitForChild("Title")  -- Reuse the "welcome" TextLabel
	
	local player = game.Players.LocalPlayer
	local correctUsername = "John"
	local correctPassword = "061001"
	
	local normalColor = Color3.fromRGB(0, 0, 0)  -- Background color for the textboxes (black)
	local wrongColor = Color3.fromRGB(255, 0, 0)  -- Red color for wrong input
	local transparency = 0.65  -- Transparency for the input boxes
	
	-- Set initial states
	welcome.BackgroundTransparency = 0.9  -- Set background transparency to 0.9
	welcome.Text = "Welcome, " .. player.Name  -- Set initial greeting message
	
	local messages = {
		"Welcome, " .. player.Name,
		"AimlockHub",
		"Hello, " .. player.Name,
		"Join our Discord"
	}
	
	local currentMessageIndex = 1  -- To keep track of which message we're on
	local isLoggedIn = false  -- Flag to track if the player is logged in
	
	-- Typewriter effect function
	local function typeWriterEffect(text, label)
		label.Text = ""  -- Start with an empty text
		for i = 1, #text do
			local currentText = text:sub(1, i)  -- Get the substring from the start to the current index
			label.Text = currentText  -- Update the text
			wait(0.1)  -- Wait before adding the next character
		end
	end
	
	-- Fade Out Text function
	local function fadeOutText(label)
		if label then
			-- Tween to fade out
			local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
			local tweenGoal = {TextTransparency = 1}
			local tween = TweenService:Create(label, tweenInfo, tweenGoal)
			tween:Play()
			tween.Completed:Connect(function()
				label.Visible = false
			end)
		end
	end
	
	-- Fade In Text function
	local function fadeInText(label)
		if label then
			-- Tween to fade in
			local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
			local tweenGoal = {TextTransparency = 0}
			label.Visible = true
			local tween = TweenService:Create(label, tweenInfo, tweenGoal)
			tween:Play()
		end
	end
	
	-- Cycle between messages every 5 seconds
	local function cycleMessages()
		while not isLoggedIn do
			-- Get the current message
			local currentMessage = messages[currentMessageIndex]
	
			-- Typewriter effect for the current message
			typeWriterEffect(currentMessage, welcome)
	
			-- Wait for the message to finish typing
			wait(#currentMessage * 0.1 + 1)  -- Wait a bit more than the typing duration
	
			-- Fade out the current message
			fadeOutText(welcome)
			wait(0.3)  -- Wait for fade out to complete
	
			-- Update the message index to the next one
			currentMessageIndex = currentMessageIndex % #messages + 1  -- Cycle through the messages
	
			-- Wait before showing the next message
			wait(1)  -- Wait a short period before showing the next message
	
			-- Fade in the next message
			fadeInText(welcome)
		end
	end
	
	-- Function to vibrate and change the color of the input box
	local function vibrateAndChangeColor(inputBox)
		-- Tween for color change to red
		local tweenInfo = TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut, 1, false, 0)
		local tweenGoal = {BackgroundColor3 = wrongColor}  -- Change background to red
		local tween = TweenService:Create(inputBox, tweenInfo, tweenGoal)
		tween:Play()
	
		-- Vibration effect: move back and forth
		for i = 1, 5 do
			local tweenMoveRight = TweenInfo.new(0.05, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut)
			local tweenGoalRight = {Position = inputBox.Position + UDim2.new(0, 5, 0, 0)}  -- Move right
			local tweenRight = TweenService:Create(inputBox, tweenMoveRight, tweenGoalRight)
			tweenRight:Play()
			wait(0.05)
	
			local tweenMoveLeft = TweenInfo.new(0.05, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut)
			local tweenGoalLeft = {Position = inputBox.Position - UDim2.new(0, 5, 0, 0)}  -- Move left
			local tweenLeft = TweenService:Create(inputBox, tweenMoveLeft, tweenGoalLeft)
			tweenLeft:Play()
			wait(0.05)
		end
	
		-- Wait for 2 seconds
		wait(2)
	
		-- Reset the color to normal
		local resetTweenGoal = {BackgroundColor3 = normalColor}  -- Reset to normal color
		local resetTween = TweenService:Create(inputBox, tweenInfo, resetTweenGoal)
		resetTween:Play()
	
		-- Reset the transparency back to 0.65
		local transparencyTween = TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut)
		local transparencyGoal = {BackgroundTransparency = transparency}
		local transparencyTweenEffect = TweenService:Create(inputBox, transparencyTween, transparencyGoal)
		transparencyTweenEffect:Play()
	end
	
	-- Start cycling messages asynchronously when the game starts
	spawn(cycleMessages)
	
	local function typeWriterEffect(label, text)
		label.Text = ""  -- Start with an empty text
		for i = 1, #text do
			local currentText = text:sub(1, i)  -- Get the substring from the start to the current index
			label.Text = currentText  -- Update the text
			wait(0.05)  -- Adjust the speed of the typewriter effect
		end
	end
	
	loginButton.MouseButton1Click:Connect(function()
		-- Check if either username or password is empty
		if usernameBox.Text == "" or passwordBox.Text == "" then
			errorLabel.Text = "Please put credentials"  -- Display error for empty fields
			errorLabel.Visible = true
		elseif usernameBox.Text == correctUsername and passwordBox.Text == correctPassword then
			-- Log the player in
			isLoggedIn = true
			errorLabel.Visible = true
	
			-- Display a loading sequence with typewriter effect
			typeWriterEffect(errorLabel, "Loading script...")
			wait(1)
	
			typeWriterEffect(errorLabel, "Loading script version...")
			wait(1)
	
			typeWriterEffect(errorLabel, "Please wait...")
			wait(1)
	
			typeWriterEffect(errorLabel, "Login Success!")
			wait(1)
	
			-- Now show the main frame
			mainFrame.Visible = true
	
			-- Optionally hide the login frame
			loginFrame.Visible = false
		else
			-- Trigger vibrate and color change for incorrect username or password
			if usernameBox.Text ~= correctUsername then
				vibrateAndChangeColor(usernameBox)
				errorLabel.Text = "Username is incorrect"  -- Indicate that the username is wrong
			elseif passwordBox.Text ~= correctPassword then
				vibrateAndChangeColor(passwordBox)
				errorLabel.Text = "Password is incorrect"  -- Indicate that the password is wrong
			end
	
			-- Show error message
			errorLabel.Visible = true
		end
	end)
	
end
coroutine.wrap(KVZOAGD_fake_script)()

